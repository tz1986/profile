<div id="jack-works">
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-sm-6">
                <h1 class="title">Works</h1>
            </div>
            <div class="col-md-4 col-sm-6">
                <div id="jack-space" class="navbar navbar-toggleable-md navbar-toggler jack-menu">
                    <a href="/website" class="right item active" id="website-tag"
                       onclick="selectMenuButton('website-tag')">
                        Website</a>
                    <a href="/app" class="right item" id="app-tag" onclick="selectMenuButton('app-tag')">
                        App</a>
                    <a class="right item" id="graphic-tag" onclick="selectMenuButton('graphic-tag')">
                        Graphic</a>
                    <a class="right item" id="photography-tag" onclick="selectMenuButton('photography-tag')">
                        Photography</a>
                    <a class="right item" id="arts-tag" onclick="selectMenuButton('arts-tag')">
                        Arts</a>
                </div>
            </div>
        </div>
        <div class="row no-gutters" style="padding-top: 30px">
            <% if(workStatus === 'app'){ %>
            <% for (let i = 1; i < 7; i++){ %>

            <div class="col-md-6">
                <% let path = '/images/app-img0' + i + '.jpg'; %>
                <img src="<%= path %>" class="img-fluid">
            </div>

            <% } %>
            <% } %>

            <% if(workStatus === 'website'){ %>
            <% for (let i = 1; i < 7; i++){ %>

            <div class="col-md-6">
                <% let path = '/images/pic0' + i + '.jpg'; %>
                <img src="<%= path %>" class="img-fluid">
            </div>
            <% } %>
            <% } %>

            <% if(workStatus === 'graphic'){ %>
            <% for (let i = 1; i < 7; i++){ %>
            <div class="col-md-6">
                <% let path = '/images/pic0' + i + '.jpg'; %>
                <img src="<%= path %>" class="img-fluid">
            </div>
            <% } %>
            <% } %>

        </div>
    </div>
</div>